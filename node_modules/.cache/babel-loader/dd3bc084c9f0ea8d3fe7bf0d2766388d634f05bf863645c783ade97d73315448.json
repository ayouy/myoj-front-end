    ...user.value,\r\n        userAvatar: response.data,\r\n    });\r\n    Message.success(\"头像上传成功！\");\r\n};\r\n\r\nconst beforeUpload = (file) => {\r\n    const isImage = file.type.startsWith(\"image/\");\r\n    if (!isImage) {\r\n        Message.error(\"请上传图片格式！\");\r\n        return false;\r\n    }\r\n    return true;\r\n};\r\n\r\nconst updateProfile = async () => {\r\n    try {\r\n        userRef.value.userAvatar = user.value.userAvatar;\r\n        await UserControllerService.updateMyUserUsingPost(userRef.value);\r\n        Message.success(\"修改成功！\");\r\n        store.commit(\"user/updateUser\", userRef.value);\r\n    } catch (error) {\r\n        Message.error(\"修改失败！\");\r\n    }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.profile-container {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    gap: 10px;\r\n}\r\n\r\n.avatar-upload {\r\n    cursor: pointer;\r\n    border-radius: 50%;\r\n    transition: all 0.3s;\r\n}\r\n\r\n.avatar-upload:hover {\r\n    opacity: 0.8;\r\n}\r\n</style>\r\n"],"mappings":"AAiCA,SAASA,QAAQ,EAAEC,GAAG,QAAQ,KAAK;AACnC,SAASC,OAAO,QAAQ,sBAAsB;AAC9C,SAASC,qBAAqB,QAAQ,oBAAoB;AAC1D,SAASC,QAAQ,QAAQ,MAAM;;;;;;;IAC/B,MAAMC,KAAK,GAAGD,QAAQ,CAAC,CAAC;IACxB,MAAME,OAAO,GAAGL,GAAG,CAAC;MAAE,GAAGI,KAAK,CAACE,KAAK,CAACC,IAAI,CAACC;IAAU,CAAC,CAAC;IACtD,MAAMD,IAAI,GAAGR,QAAQ,CAAC,MAAMK,KAAK,CAACE,KAAK,CAACC,IAAI,CAACC,SAAS,CAAC;IAGvD,MAAMC,mBAAmB,GAAIC,QAAQ,IAAK;MACtCH,IAAI,CAACI,KAAK,CAACC,UAAU,GAAGF,QAAQ,CAACG,GAAG;MACpCT,KAAK,CAACU,MAAM,CAAC,iBAAiB,EAAE;QAC5B,GAAGP,IAAI,CAACI,KAAK;QACbC,UAAU,EAAEF,QAAQ,CAACK;MACzB,CAAC,CAAC;MACFd,OAAO,CAACe,OAAO,CAAC,SAAS,CAAC;IAC9B,CAAC;IAED,MAAMC,YAAY,GAAIC,IAAI,IAAK;MAC3B,MAAMC,OAAO,GAAGD,IAAI,CAACE,IAAI,CAACC,UAAU,CAAC,QAAQ,CAAC;MAC9C,IAAI,CAACF,OAAO,EAAE;QACVlB,OAAO,CAACqB,KAAK,CAAC,UAAU,CAAC;QACzB,OAAO,KAAK;MAChB;MACA,OAAO,IAAI;IACf,CAAC;IAED,MAAMC,aAAa,GAAG,MAAAA,CAAA,KAAY;MAC9B,IAAI;QACAlB,OAAO,CAACM,KAAK,CAACC,UAAU,GAAGL,IAAI,CAACI,KAAK,CAACC,UAAU;QAChD,MAAMV,qBAAqB,CA