serControllerService.updateProfile({\r\n            ...user\r\n        });\r\n        Message.success(\"修改成功！\");\r\n    } catch (error) {\r\n        Message.error(\"修改失败！\");\r\n    }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.profile-container {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    gap: 10px;\r\n}\r\n\r\n.avatar-upload {\r\n    cursor: pointer;\r\n    border-radius: 50%;\r\n    transition: all 0.3s;\r\n}\r\n\r\n.avatar-upload:hover {\r\n    opacity: 0.8;\r\n}\r\n</style>\r\n"],"mappings":"AAiCA,SAASA,GAAG,QAAQ,KAAK;AACzB,SAASC,OAAO,QAAQ,sBAAsB;AAC9C,SAASC,qBAAqB,QAAQ,WAAW;AACjD,SAASC,QAAQ,QAAQ,MAAM;;;;;;;IAC/B,MAAMC,KAAK,GAAGD,QAAQ,CAAC,CAAC;IACxB,MAAME,IAAI,GAAGC,QAAQ,CAAC,MAAMF,KAAK,CAACG,KAAK,CAACF,IAAI,CAACG,SAAS,CAAC;IAEvD,MAAMC,mBAAmB,GAAIC,QAAQ,IAAK;MACtCL,IAAI,CAACM,KAAK,CAACC,UAAU,GAAGF,QAAQ,CAACG,GAAG;MACpCT,KAAK,CAACU,MAAM,CAAC,iBAAiB,EAAC;QAAE,GAAGT,IAAI,CAACM,KAAK;QAAEC,UAAU,EAAEF,QAAQ,CAACG;MAAI,CAAC,CAAC;MAC3EZ,OAAO,CAACc,OAAO,CAAC,SAAS,CAAC;IAC9B,CAAC;IAED,MAAMC,YAAY,GAAIC,IAAI,IAAK;MAC3B,MAAMC,OAAO,GAAGD,IAAI,CAACE,IAAI,CAACC,UAAU,CAAC,QAAQ,CAAC;MAC9C,IAAI,CAACF,OAAO,EAAE;QACVjB,OAAO,CAACoB,KAAK,CAAC,UAAU,CAAC;QACzB,OAAO,KAAK;MAChB;MACA,OAAO,IAAI;IACf,CAAC;IAED,MAAMC,aAAa,GAAG,MAAAA,CAAA,KAAY;MAC9B,IAAI;QACA,MAAMpB,qBAAqB,CAACoB,aAAa,CAAC;UACtC,GAAGjB;QACP,CAAC,CAAC;QACFJ,OAAO,CAACc,OAAO,CAAC,OAAO,CAAC;MAC5B,CAAC,CAAC,OAAOM,KAAK,EAAE;QACZpB,OAAO,CAACoB,KAAK,CAAC,OAAO,CAAC;MAC1B;IACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}