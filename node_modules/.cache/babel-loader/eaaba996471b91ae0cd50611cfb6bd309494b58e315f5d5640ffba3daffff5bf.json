{"ast":null,"code":"import { EditorView, basicSetup } from \"@codemirror/basic-setup\";\nimport { EditorState } from \"@codemirror/state\";\nimport { javascript } from \"@codemirror/lang-javascript\";\nexport default {\n  name: \"codemirrorDemo\",\n  data() {\n    return {\n      code: \"const a = 10;\",\n      editorView: null\n    };\n  },\n  methods: {\n    createEditor() {\n      const startState = EditorState.create({\n        doc: this.code,\n        extensions: [basicSetup, javascript(), EditorView.updateListener.of(update => {\n          if (update.docChanged) {\n            this.code = update.state.doc.toString();\n            console.log(\"Updated code:\", this.code);\n          }\n        })]\n      });\n      this.editorView = new EditorView({\n        state: startState,\n        parent: this.$refs.cmEditor\n      });\n      console.log(\"Editor created:\", this.editorView);\n    },\n    destroyEditor() {\n      if (this.editorView) {\n        this.editorView.destroy();\n        this.editorView = null;\n        console.log(\"Editor destroyed.\");\n      }\n    }\n  },\n  mounted() {\n    this.createEditor();\n  },\n  beforeDestroy() {\n    this.destroyEditor();\n  }\n};","map":{"version":3,"names":["EditorView","basicSetup","EditorState","javascript","name","data","code","editorView","methods","createEditor","startState","create","doc","extensions","updateListener","of","update","docChanged","state","toString","console","log","parent","$refs","cmEditor","destroyEditor","destroy","mounted","beforeDestroy"],"sources":["D:\\Nodejs\\oj-frontend\\src\\views\\HomeView.vue"],"sourcesContent":["<template>\n  <div ref=\"cmEditor\" class=\"editor\"></div>\n</template>\n\n<script>\nimport { EditorView, basicSetup } from \"@codemirror/basic-setup\";\nimport { EditorState } from \"@codemirror/state\";\nimport { javascript } from \"@codemirror/lang-javascript\";\n\nexport default {\n  name: \"codemirrorDemo\",\n  data() {\n      return {\n          code: \"const a = 10;\",\n          editorView: null,\n      };\n  },\n  methods: {\n      createEditor() {\n          const startState = EditorState.create({\n              doc: this.code,\n              extensions: [\n                  basicSetup,\n                  javascript(),\n                  EditorView.updateListener.of((update) => {\n                      if (update.docChanged) {\n                          this.code = update.state.doc.toString();\n                          console.log(\"Updated code:\", this.code);\n                      }\n                  }),\n              ],\n          });\n\n          this.editorView = new EditorView({\n              state: startState,\n              parent: this.$refs.cmEditor,\n          });\n\n          console.log(\"Editor created:\", this.editorView);\n      },\n      destroyEditor() {\n          if (this.editorView) {\n              this.editorView.destroy();\n              this.editorView = null;\n              console.log(\"Editor destroyed.\");\n          }\n      },\n  },\n  mounted() {\n      this.createEditor();\n  },\n  beforeDestroy() {\n      this.destroyEditor();\n  },\n};\n</script>\n\n<style>\n.editor {\n  height: 400px;\n  border: 1px solid #ccc;\n  background-color: #2d2d2d;\n  color: #ffffff;\n}\n</style>\n"],"mappings":"AAKA,SAASA,UAAU,EAAEC,UAAS,QAAS,yBAAyB;AAChE,SAASC,WAAU,QAAS,mBAAmB;AAC/C,SAASC,UAAS,QAAS,6BAA6B;AAExD,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,IAAI,EAAE,eAAe;MACrBC,UAAU,EAAE;IAChB,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,YAAYA,CAAA,EAAG;MACX,MAAMC,UAAS,GAAIR,WAAW,CAACS,MAAM,CAAC;QAClCC,GAAG,EAAE,IAAI,CAACN,IAAI;QACdO,UAAU,EAAE,CACRZ,UAAU,EACVE,UAAU,CAAC,CAAC,EACZH,UAAU,CAACc,cAAc,CAACC,EAAE,CAAEC,MAAM,IAAK;UACrC,IAAIA,MAAM,CAACC,UAAU,EAAE;YACnB,IAAI,CAACX,IAAG,GAAIU,MAAM,CAACE,KAAK,CAACN,GAAG,CAACO,QAAQ,CAAC,CAAC;YACvCC,OAAO,CAACC,GAAG,CAAC,eAAe,EAAE,IAAI,CAACf,IAAI,CAAC;UAC3C;QACJ,CAAC,CAAC;MAEV,CAAC,CAAC;MAEF,IAAI,CAACC,UAAS,GAAI,IAAIP,UAAU,CAAC;QAC7BkB,KAAK,EAAER,UAAU;QAC